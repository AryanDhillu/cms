{
  "name": "worker",
  "version": "1.0.0",
  "main": "dist/publish.worker.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/publish.worker.js",
    "dev": "ts-node src/publish.worker.ts",
    "copy-schema": "node -e \"require('fs').copyFileSync('../api/prisma/schema.prisma', './prisma/schema.prisma')\"",
    "generate": "npm run copy-schema && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.6.6",
    "prisma": "^7.2.0"
  }
}
